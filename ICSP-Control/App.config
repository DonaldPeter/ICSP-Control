<?xml version="1.0" encoding="utf-8"?>
<configuration>

  <configSections>
    <section name="log4net" type="Log4Net.Config.Log4NetConfigurationSectionHandler, Log4Net"/>
    <sectionGroup name="userSettings" type="System.Configuration.UserSettingsGroup, System, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089">
      <section name="ICSPControl.Properties.Settings" type="System.Configuration.ClientSettingsSection, System, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" allowExeDefinition="MachineToLocalUser" requirePermission="false"/>
    </sectionGroup>
  </configSections>

  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7.2"/>
  </startup>

  <log4net>

    <appender name="LogFileAppender" type="Log4Net.Appender.FileAppender">
      <file type="Log4Net.Util.PatternString" value="IcspControl.log"/>
      <appendToFile value="false"/>
      <lockingModel type="Log4Net.Appender.FileAppender+MinimalLock"/>
      <layout type="Log4Net.Layout.IndentationPatternLayout">
        <header value=""/>
        <footer value=""/>
        <!--conversionPattern value="%-22date [%5thread] [%property{IpAddress}] %-5level - %indentation%message%newline"/-->
        <conversionPattern value="%-22date %-5level - %indentation%message%newline"/>
      </layout>
    </appender>

    <root>
      <!-- ALL DEBUG INFO WARN ERROR FATAL OFF -->
      <level value="DEBUG"/>
      <appender-ref ref="LogFileAppender"/>
      <appender-ref ref="TraceAppender"/>
    </root>

  </log4net>

  <userSettings>
    <ICSPControl.Properties.Settings>
      <setting name="AmxHost" serializeAs="String">
        <value>172.16.16.101</value>
      </setting>
      <setting name="AmxPort" serializeAs="String">
        <value>1319</value>
      </setting>
      <setting name="AutoConnect" serializeAs="String">
        <value>False</value>
      </setting>
      <setting name="PhysicalDeviceNumber" serializeAs="String">
        <value>15000</value>
      </setting>
      <setting name="PhysicalDevicePortCount" serializeAs="String">
        <value>5</value>
      </setting>
      <setting name="PhysicalDeviceVersion" serializeAs="String">
        <value>v1.00.00</value>
      </setting>
      <setting name="PhysicalDeviceName" serializeAs="String">
        <value>ICSP Window</value>
      </setting>
      <setting name="PhysicalDeviceManufacturer" serializeAs="String">
        <value>ICSP Manufacturer</value>
      </setting>
      <setting name="PhysicalDeviceSerialNumber" serializeAs="String">
        <value>0000000000000000</value>
      </setting>
      <setting name="PhysicalDeviceManufactureId" serializeAs="String">
        <value>1</value>
      </setting>
      <setting name="PhysicalDeviceDeviceId" serializeAs="String">
        <value>1</value>
      </setting>
      <setting name="PhysicalDeviceFirmwareId" serializeAs="String">
        <value>1</value>
      </setting>
      <setting name="LastSendText" serializeAs="String">
        <value/>
      </setting>
      <setting name="PhysicalDeviceAutoCreate" serializeAs="String">
        <value>False</value>
      </setting>
    </ICSPControl.Properties.Settings>
  </userSettings>
</configuration>
